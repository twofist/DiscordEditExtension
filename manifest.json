{

    "manifest_version": 2,
    "name": "Discord Edit",
    "version": "1.0",
  
    "description": "allows editing text client side",
  
    "icons": {
      "48": "icons/edit.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://discord.com/*"],
        "js": ["edit.js"]
      }
    ]
  }
  